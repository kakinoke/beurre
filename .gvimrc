scriptencoding UTF-8
set showtabline=2
"autocmd GUIEnter * set transparency=220
set imdisable
set guioptions-=T
set antialias
set tabstop=2
set shiftwidth=2
set noexpandtab
set number
set nobackup
set visualbell t_vb=
set nowrapscan
set columns=150
set lines=68
set background=dark
colorscheme solarized
set guifont=Ricty:h16
let g:vimfiler_enable_auto_cd = 1
